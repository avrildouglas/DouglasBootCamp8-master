package escape;
public class EscapeTheSequences {
	 public static void main(String[] args) { 
	    System.out.print("\t");
	    System.out.print("*********\r\t\n\r\t");
	    System.out.print("*\t*\r\n\t\r");
	    System.out.print("*\t*\r\n\t\r");
	    System.out.print("*\t*\r\n\t\r");
	    System.out.print("*\t*\r\n\r\t");
	    System.out.print("*********");
}
}