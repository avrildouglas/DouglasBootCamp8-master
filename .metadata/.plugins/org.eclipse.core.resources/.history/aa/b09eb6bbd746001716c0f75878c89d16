
public class AnimalApp {

	public static void main(String[] args) {

		Animal a = new Animal();
		print(a.eat());
		print(a.sleep());
		print(a.breathe());

		Cat c = new Cat();
		print(c.eat());
		print(c.sleep());
		print(c.purr());
		print(c.breathe());
		

		Bird b = new Bird();
		print(b.eat());
		print(b.sleep());
		print(b.fly());
		print(b.breathe());
		
		Cow w = new Cow();
		print(w.eat());
		print(w.moo());
		print(w.drinks());
		print(w.breathe());

	}
	private static void print(String s){
		System.out.println(s);
	}
}
