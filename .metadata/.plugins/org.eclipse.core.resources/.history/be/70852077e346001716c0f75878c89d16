package newAnimalExperiment;

/*
* All Birds are Animals but all Animals are not Birds
*/
public class Cat extends Animal  {
	
public String theAnimal="Bird";

	public Cat() {
		super();
		System.out.println("A " + theAnimal + " is hatched");
	}
		
	public String sleep(){
		
		//if I want to call the method that's actually in the parent class
	//	theAnimal = super.sleep();
		return "A " + theAnimal + " sleeps soundly...";
		}


	public String eat(){
		
	//		//if I want to call the method that's actually in the parent class
			return "A " + theAnimal + " eats....";
		}

	
	public String breathe(){
		
	//		//if I want to call the method that's actually in the parent class
			return "A " + theAnimal + " breathes...";
		}
}
