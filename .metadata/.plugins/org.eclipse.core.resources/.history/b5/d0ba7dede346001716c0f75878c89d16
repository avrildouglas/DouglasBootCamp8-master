package newAnimalExperiment;

/*
* All Cat are Animals but all Animals are not Birds
*/
public class Chimpanzee extends Primate  {
	
public String theAnimal="Gorilla";

	public Chimpanzee() {
		super();
		System.out.println("I am now a " + theAnimal);
	}
		
	public String sleep(){
		
		//if I want to call the method that's actually in the parent class
	//	theAnimal = super.sleep();
		return "A " + theAnimal + " sleeps...";
		}


	public String eat(){
		
	//		//if I want to call the method that's actually in the parent class
			return "A " + theAnimal + " eats...";
		}
	
	
	public String breathe(){
		
	//		//if I want to call the method that's actually in the parent class
			return "A " + theAnimal + " breathes...";
		}
}